<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="food_name_style">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#222831</item>
    </style>
    <style name="food_description_style">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#AAAAAD</item>
    </style>
    <style name="buy_button_syle">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#FD3A69</item>
    </style>
    <style name="rounded_square">
        <item name="cornerSize">20%</item>
    </style>
    <style name="chip_filter" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="android:textSize">13sp</item>
        <item name="chipCornerRadius">4dp</item>
        <item name="chipBackgroundColor">@color/chip_select_color</item>
    </style>
    <style name="bottom_menu_button_style" parent="Widget.AppCompat.Button.Borderless">
        <item name="iconGravity">top</item>
        <item name="iconSize">24dp</item>
        <item name="iconPadding">4dp</item>
        <item name="android:textSize">12sp</item>
        <item name="textAllCaps">false</item>
        <item name="iconTint">@color/not_active_text</item>
        <item name="android:textColor">@color/not_active_text</item>
    </style>
</resources>